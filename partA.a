ARRADD EQU 0x66;
PTRADD EQU 0x65;
PTR: .data 1 PTRADD;
ONE: .data 1 1;
loadi ARRADD;
sta PTRADD;
LOOP1:
ipa;
jmpp ADD_TO_ARR;
jmp ADD_TO_ARR_DONE;
ADD_TO_ARR:
sta 0xFF;
a2m PTRADD;
lda PTRADD;
add ONE;
sta PTRADD;
lda 0xFF;
ADD_TO_ARR_DONE:
jmpnz LOOP1;
lda PTRADD;
sub ONE;
sta PTRADD;
LOOP2:
m2a PTRADD;
opa;
lda PTRADD;
sub ONE;
sta PTRADD;
sub PTR;
jmpp LOOP2;
stop;
 