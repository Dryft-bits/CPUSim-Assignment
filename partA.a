PTRADD EQU 0x65;
ARRADD EQU 0x66;
PTR: .data 1 PTRADD;
ONE: .data 1 1;
addi ARRADD;
sta PTRADD;
LOOP1:
ipa;
jmpp ADD_TO_ARR;
jmp ADD_TO_ARR_DONE;
ADD_TO_ARR:
sta 0xFF;
a2m PTRADD;
lda PTRADD;
addi 1;
sta PTRADD;
lda 0xFF;
ADD_TO_ARR_DONE:
jmpnz LOOP1;
lda PTRADD;
sub ONE;
sta PTRADD;
LOOP2:
m2a PTRADD;
opa;
lda PTRADD;
sub ONE;
sta PTRADD;
sub PTR;
jmpp LOOP2;
stop;
 
